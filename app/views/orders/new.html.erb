<%#= debug @user %>
<%#= debug @address %>
<%#= debug @order %>
<%- model_class = Order -%>
<div class="row">
	<div class="span9">
		<div class="page-header">
		  <h1><%=t '.title', :default => t('helpers.titles.new', :model => model_class.model_name.human,
		                                 :default => "New #{model_class.model_name.human}") %></h1>
		</div>
		<%= debug flash %>
		<div id='order_form'>
			<%= render :partial => 'form' %>
		</div>
	</div>
	<div class="span3">
		<div class="affix-top" data-spy="affix" data-offset-top="50">
			<div class="">
				<%= render @cart %>
			</div>
		</div>
	</div>
</div>