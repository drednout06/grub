<script type="text/javascript" charset="utf-8">

	$.datepicker.regional["<%= I18n.locale.to_s %>"] = {
		closeText: "<%= t 'datepicker.closeText' %>",
		prevText: "<%=raw t 'datepicker.prevText' %>",
		nextText: "<%=raw t 'datepicker.nextText' %>",
		currentText: "<%= t 'datepicker.currentText' %>",
		monthNames: [<%=raw t('date.month_names_nominative').map(&:inspect).join(", ") %>],
		monthNamesShort: [<%=raw t('date.abbr_month_names')[1..12].map(&:inspect).join(", ") %>],
		dayNames: [<%=raw t('date.day_names').map(&:inspect).join(", ") %>],
		dayNamesShort: [<%=raw t('date.abbr_day_names').map(&:inspect).join(", ") %>],
		dayNamesMin: [<%=raw t('date.abbr_day_names').map(&:inspect).join(", ") %>],
		weekHeader: "<%= t 'datepicker.weekHeader' %>",
		dateFormat: 'dd.mm.yy',
		firstDay: 1,
		isRTL: false,
		showMonthAfterYear: false,
		yearSuffix: ''
	};
	$.datepicker.setDefaults($.datepicker.regional["<%= I18n.locale.to_s %>"]);


	$.timepicker.regional["<%= I18n.locale.to_s %>"] = {
		timeOnlyTitle: "<%= t 'timepicker.timeOnlyTitle' %>",
		timeText: "<%= t 'timepicker.timeText' %>",
		hourText: "<%= t 'timepicker.hourText' %>",
		minuteText: "<%= t 'timepicker.minuteText' %>",
		secondText: "<%= t 'timepicker.secondText' %>",
		millisecText: "<%= t 'timepicker.millisecText' %>",
		timezoneText: "<%= t 'timepicker.timezoneText' %>",
		currentText: "<%= t 'timepicker.currentText' %>",
		closeText: "<%= t 'timepicker.closeText' %>",
		timeFormat: 'hh:mm tt',
		amNames: ['AM', 'A'],
		pmNames: ['PM', 'P'],
		ampm: false,
		isRTL: false
	};
	$.timepicker.setDefaults($.timepicker.regional["<%= I18n.locale.to_s %>"]);


</script>