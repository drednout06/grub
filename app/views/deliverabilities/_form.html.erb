<%= simple_form_for @deliverability, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :restaurant_id %>
  <%= f.input :district_id %>
  <%= f.input :fee %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                deliverabilities_path, :class => 'btn' %>
  </div>
<% end %>
